algoritmo "Tabuada"

var

numero, resultado: inteiro

        i:inteiro
        
        inicio
        
        Escreva ("digite o número inteiro para ver sua tabuada ")

        leia (numero)
        
        para i de 1 ate 20 faca


resultado <- numero * i

         escreval(numero, "x", i, " = ", resultado)
         
fimpara
         

         fimalgoritmo